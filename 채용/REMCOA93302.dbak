    <javascript id="f57f56e9-b3c8-4d86-9219-28794d930f4a"><![CDATA[function() {
  var dirtyCountgridMaster1 = self.dataSourceUN_gridMaster1_M.getDirtyDataCount();
  var dirtyCountgridDetail1 = self.dataSourceUN_gridDetail1_M.getDirtyDataCount();
  if (dirtyCountgridMaster1 == 0 && dirtyCountgridDetail1 == 0) {
    dews.alert('변경된 데이터가 없습니다.');
  }
  if (self.gridMaster1.validate().result == false) {
    dews.alert('그리드 gridMaster1에 필수 값이 입력되지 않은 항목이 있습니다.');
  }
  dews.confirm('수정 된 데이터를 저장 하시겠습니까?', 'question').yes(function () {
    dews.api.post(dews.url.getApiUrl('CI', 'CiUsrdeftblService', 'usrdeftbl_data_milti_save'), {
      async: true,
      data: {
        module_cd: dews.ui.page.menu.module,
        Usrdeftbl_Data_Mst: JSON.stringify(self.dataSourceUN_gridMaster1_M.getDirtyData()),
        Usrdeftbl_Data_Dtl: JSON.stringify(self.dataSourceUN_gridDetail1_M.getDirtyData())
      }
    }).done(function (data) {
      dews.ui.snackbar.ok('저장이 완료되었습니다.');
      self.dataSourceUN_gridMaster1_M.read();
    }).fail(function () {
      dews.error('저장에 실패 했습니다.');
    });
  });
}
]]></javascript>
